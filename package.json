{
  "name": "@douglasneuroinformatics/libui",
  "type": "module",
  "version": "2.10.0",
  "packageManager": "pnpm@9.3.0",
  "description": "Generic UI components for DNP projects, built using React and Tailwind CSS",
  "author": "Joshua Unrau",
  "license": "Apache-2.0",
  "homepage": "https://github.com/DouglasNeuroInformatics/libui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/DouglasNeuroInformatics/libui"
  },
  "bugs": {
    "url": "https://github.com/DouglasNeuroInformatics/libui/issues"
  },
  "keywords": [
    "React",
    "TailwindCSS"
  ],
  "exports": {
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./styles/*": "./dist/styles/*",
    "./tailwind.config.cjs": "./tailwind.config.cjs",
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    }
  },
  "files": [
    "!/src/**/*.spec.*",
    "!/src/**/*.stories.*",
    "!/src/**/*.test.*",
    "/dist",
    "/src",
    "/tailwind.config.cjs",
    "/tailwind.config.d.cts"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -b tsconfig.build.json && cp -r src/styles dist",
    "format": "prettier --write src",
    "format:translations": "find src/translations -name '*.json' -exec pnpm exec sort-json {} \\;",
    "lint": "tsc && eslint --fix src",
    "prepare": "husky",
    "storybook": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.6"
  },
  "dependencies": {
    "@douglasneuroinformatics/libjs": "^0.6.0",
    "@douglasneuroinformatics/libui-form-types": "^0.10.3",
    "@headlessui/tailwindcss": "^0.2.1",
    "@heroicons/react": "^2.1.5",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.5.2",
    "i18next": "23.x",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.438.0",
    "react-error-boundary": "^4.0.13",
    "react-i18next": "^14.1.2",
    "react-resizable-panels": "^2.1.2",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.3.1",
    "type-fest": "^4.26.0",
    "vaul": "^0.9.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@douglasneuroinformatics/eslint-config": "^5.1.1",
    "@douglasneuroinformatics/prettier-config": "^0.0.1",
    "@douglasneuroinformatics/semantic-release": "^0.0.1",
    "@douglasneuroinformatics/tsconfig": "^1.0.2",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/components": "^8.2.9",
    "@storybook/icons": "^1.2.10",
    "@storybook/manager-api": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/theming": "^8.2.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "happy-dom": "^14.12.0",
    "husky": "^9.1.5",
    "jsdom": "25.0.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sort-json": "^2.0.1",
    "storybook": "^8.2.9",
    "storybook-react-i18next": "^3.1.7",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "vite": "5.4.3",
    "vitest": "^2.0.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": [
      "@douglasneuroinformatics/semantic-release"
    ]
  }
}
